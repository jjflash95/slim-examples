trait Array {
    fn contains(i) {
        return for item in self {
            if item == i {
                break true
            }
        }
    }
}

impl Array for sequence

fib = fn(n) {
    return if n == 0 {
        0
    } else if [1, 2].contains(n) {
        1
    } else {
        fib(n - 2) + fib(n - 1)
    }
}

print(fib(10))
